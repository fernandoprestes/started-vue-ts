{
  "name": "vuets-start",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "dev:host": "vite --host --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore"
  },
  "lint-staged": {
    "*src/**/*.{js,ts,vue}": [
      "eslint --fix --ignore-path .gitignore --max-warnings=0",
      "prettier --write"
    ],
    "*src/**/*.{css,scss,vue}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^1.3.4",
    "vue": "^3.2.47",
    "vue-query": "^1.26.0",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify-json/ph": "^1.1.4",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@unocss/preset-icons": "^0.50.1",
    "@unocss/preset-rem-to-px": "^0.50.1",
    "@unocss/reset": "^0.50.1",
    "@unocss/transformer-variant-group": "^0.50.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.5",
    "unocss": "^0.50.1",
    "vite": "^4.1.4",
    "vite-plugin-inspect": "^0.7.15",
    "vite-plugin-vue-inspector": "^3.3.2",
    "vue-tsc": "^1.2.0"
  }
}
