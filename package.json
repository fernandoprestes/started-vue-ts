{
	"name": "vuets-start",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite --open",
		"dev:host": "vite --host --open",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
		"format:check": "prettier --check .",
		"lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore"
	},
	"lint-staged": {
		"*src/**/*.{js,ts,vue}": [
			"eslint --fix --ignore-path .gitignore --max-warnings=0",
			"prettier --write"
		],
		"*src/**/*.{css,scss,vue}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"axios": "^1.4.0",
		"vue": "^3.3.4",
		"vue-query": "^1.26.0",
		"vue-router": "^4.2.2"
	},
	"devDependencies": {
		"@iconify-json/ph": "^1.1.5",
		"@types/node": "^20.2.5",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"@unocss/preset-icons": "^0.53.1",
		"@unocss/preset-rem-to-px": "^0.53.1",
		"@unocss/reset": "^0.53.1",
		"@unocss/transformer-variant-group": "^0.53.1",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.14.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.8",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"sass": "^1.63.0",
		"sass-loader": "^13.3.1",
		"typescript": "^5.1.3",
		"unocss": "^0.53.1",
		"vite": "^4.3.9",
		"vite-plugin-inspect": "^0.7.28",
		"vite-plugin-vue-inspector": "^3.4.2",
		"vue-tsc": "^1.6.5"
	}
}
