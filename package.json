{
  "name": "vuets-start",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "vite --open",
    "dev:host": "vite --host --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore",
    "stylelint": "stylelint \"**/*.{vue,scss}\" --fix",
    "stylelint:check": "stylelint \"**/*.{vue,scss}\""
  },
  "dependencies": {
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@hint/connector-local": "^3.2.22",
    "@hint/formatter-codeframe": "^3.1.31",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "hint": "^7.1.1",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.3",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-sass-render-errors": "^3.0.0",
    "stylelint-selector-no-empty": "^1.0.9",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vue-tsc": "^0.38.4"
  }
}
